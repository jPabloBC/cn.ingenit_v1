{
  "build": {
    "beforeDevCommand": "",
    "beforeBuildCommand": "",
    "devPath": "../ui",
    "distDir": "../ui",
    "withGlobalTauri": true
  },
    "package": {
    "productName": "CN IngenIT",
    "version": "1.0.0.6"
  },
  "tauri": {
    "allowlist": {
      "all": true
    },
      "updater": {
        "active": false,
        "dialog": true,
        "endpoints": [
          "https://api.github.com/repos/<OWNER>/<REPO>/releases/latest"
        ]
      },
      "bundle": {
      "active": true,
      "targets": "all",
      "identifier": "com.formautomation.app",
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ],
      "resources": ["../automation"],
      "externalBin": []
    },
    "security": {
      "csp": null
    },
    "windows": [
      {
        "fullscreen": false,
        "resizable": true,
        "title": "CN IngenIT",
        "width": 800,
        "height": 600,
        "center": true
      }
    ]
  }
}
