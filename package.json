{
  "name": "cn-ingenit",
  "version": "1.0.0-6",
  "description": "Desktop app for automating web form filling using Tauri + Node.js + Playwright",
  "private": true,
  "scripts": {
    "tauri": "tauri",
    "dev": "node ./scripts/sync-version.js && tauri dev",
    "build": "node ./scripts/sync-version.js && tauri build",
    "build:automation": "cd automation && npm ci && npx playwright install --with-deps"
  },
  "devDependencies": {
    "@tauri-apps/cli": "^1.5.9",
    "@types/express": "^5.0.6",
    "@types/node": "^25.0.3",
    "vite": "^7.3.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.89.0",
    "@tauri-apps/api": "^1.5.0",
    "express": "^5.2.1"
  }
}
